<div class="container stores-container">
    <div class="row row-stores align-items-center">
        <% shops.forEach(function(shop, i) { %>
            <div class="col col-stores my-auto">
                <div style="background-color: #eaf2ef" class="row shop-container">
                    <div class="col-xs-4">
                        <img class="img img-thumbnail rounded" src="<%=shop.productImage%>" alt="...">
                    </div>
                    <div class="col-xs-8">

                        <p class="card-text"><a href="/getShopVendorById/<%=shop._id%>"><%= shop.vendorName%></a></p>
                    </div>
                </div>
             </div>
        <% }) %>
    </div>
</div>
